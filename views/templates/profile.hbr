<div class="row-fluid">
	<div class="span6">
		<h1>{{canonicalName}}</h1>
		<p>Signed up on {{date}}</p>
		<p>
			<a class="btn" {{action goClassify href=true}}>
				Do some classifying
			</a>
		</p>
		<br>
		<p>Added {{total}} contact{{#unless_eq total compare=1}}s{{/unless_eq}} so far.</p>
		<ul>
			{{#each contacts}}
				<li><a {{action goContact this href=true}}>{{name}}</a></li>
			{{/each}}
		</ul>
	</div>
	<div class="span6">
	</div>
</div>
