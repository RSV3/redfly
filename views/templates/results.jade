.result
	| {{#each result in controller}}
	{{view App.ResultView contentBinding="result"}}
	| {{/each}}

	ul.pager
	| {{#if hasPrevious}}
	li.previous
		| <a href="#" {{action previousPage target="controller"}}>&larr; Newer</a>
	| {{/if}}
	| {{#if hasNext}}
	li.next
		| <a href="#" {{action nextPage target="controller"}}>Older &rarr;</a>
	| {{/if}}

