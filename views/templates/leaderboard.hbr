<div class="row-fluid">
	<h1>All Users</h1>
	<table class="table table-hover">
		<thead>
			<tr>
				<th>Name</th>
				<th>Email</th>
				<th>Signed Up</th>
			</tr>
		</thead>
		<tbody>
			{{#each content}}
				<tr {{action goProfile this href=true}}>
					<td>{{name}}</td>
					<td>{{email}}</td>
					<td>{{date}}</td>
				</tr>
			{{/each}}
		</tbody>
	</table>
</div>
