.row-fluid
	h1 All Contacts
	table.table.table-hover
		thead
			tr
				th Names
				th Emails
				th Added On
				th Added By
				th Known To
		tbody
			| {{#each controller}}
			| <tr {{action goContact this href=true}}>
			td {{canonicalName}}
			td {{email}}
			td {{added}}
			td {{addedBy.canonicalName}}
			td {{knows.firstObject.canonicalName}}
			| </tr>
			| {{/each}}
