.row-fluid
	.span6
		h1 {{canonicalName}}
		p Signed up on {{date}}
		p
			| <a class="btn" {{action goClassify href=true}}>Do some classifying</a>
		br
		p Added {{total}} contact{{#unless_eq total compare=1}}s{{/unless_eq}} so far.
		ul
			| {{#each contacts}}
			li
				| <a {{action goContact this href=true}}>{{name}}</a>
			| {{/each}}
	.span6
