	.well
		background-color: white


.tagger
	border 1px solid #C6C6C6
	.tag
		margin 4px 0 0 4px
		display inline-block
		background-color #DEE7F8
		border 1px solid #CAD8F3
		border-radius 8px 4px 4px 8px
		padding-left 8px
		.name
			color #666
			margin-right 4px
		.close
			float none
			font-size inherit
			border-left 1px dotted #1323d6
			padding-left 3px
			padding-right 3px
	.new-tag-form
		margin 4px 0 0 4px
		height 25px
		display inline
		.new-tag
			border none
			box-shadow none
			width auto
			margin 0


